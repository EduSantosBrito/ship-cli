{
  "name": "ship-monorepo",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.14.0",
  "scripts": {
    "build": "pnpm -r build",
    "check": "pnpm -r check",
    "lint": "pnpm -r lint",
    "format": "pnpm -r format",
    "test": "pnpm -r test",
    "ship": "tsx packages/cli/src/bin.ts",
    "publish:all": "pnpm build && pnpm -r publish --access public"
  },
  "devDependencies": {
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
  }
}
