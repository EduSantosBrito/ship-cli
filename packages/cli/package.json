{
  "name": "@ship-cli/core",
  "version": "0.1.10",
  "type": "module",
  "license": "MIT",
  "description": "Linear + jj workflow CLI for AI agents",
  "repository": {
    "type": "git",
    "url": "https://github.com/EduSantosBrito/ship-cli.git",
    "directory": "packages/cli"
  },
  "homepage": "https://github.com/EduSantosBrito/ship-cli#readme",
  "bugs": {
    "url": "https://github.com/EduSantosBrito/ship-cli/issues"
  },
  "author": "Eduardo Santos de Brito",
  "keywords": [
    "linear",
    "jj",
    "jujutsu",
    "cli",
    "ai",
    "opencode",
    "task-management",
    "stacked-diffs",
    "effect"
  ],
  "bin": {
    "ship": "./dist/bin.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "tsx src/bin.ts",
    "build": "tsup",
    "clean": "rimraf dist/*",
    "check": "tsc -b tsconfig.json",
    "lint": "oxlint",
    "format": "oxfmt --write src/",
    "format:check": "oxfmt src/",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "prepare": "effect-language-service patch --module typescript"
  },
  "devDependencies": {
    "@effect/cli": "^0.73.0",
    "@effect/language-service": "^0.64.1",
    "@effect/platform": "^0.94.1",
    "@effect/platform-node": "^0.104.0",
    "@effect/vitest": "^0.27.0",
    "@linear/sdk": "^68.1.0",
    "@types/node": "^25.0.3",
    "@types/ws": "^8.18.1",
    "@vitest/coverage-v8": "^4.0.16",
    "effect": "^3.19.14",
    "oxfmt": "^0.21.0",
    "oxlint": "^1.36.0",
    "rimraf": "^6.1.2",
    "tsup": "^8.2.4",
    "tsx": "^4.19.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16",
    "yaml": "^2.7.0"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "@notionhq/client": "^5.6.0",
    "ws": "^8.18.3"
  }
}
